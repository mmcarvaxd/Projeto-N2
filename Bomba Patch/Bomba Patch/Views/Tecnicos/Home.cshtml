
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<main>
    <div class="full-width">
        <div class="option-title">
            <div class="options">
                <div class="btn-voltar noselect">
                    <i class="fas fa-arrow-left noselect"></i>
                    <p class="noselect">Voltar</p>
                </div>
                <div class="noselect">
                    <p class="noselect">Técnicos</p>
                </div>
            </div>
        </div>
        <div class="cards-home">
            <div class="card-home mg-right-10px">
                <div>
                    <p class="font-title">Criar Técnico</p>
                </div>
                <div class="img-home-div">
                    <img draggable="false" src="~/images/addtecnico.png" alt="Add tecnico" />
                </div>
            </div>
            <div class="card-home">
                <div>
                    <p class="font-title">Exibir Técnicos</p>
                </div>
                <div class="img-home-div">
                    <img draggable="false" src="~/images/seetecnico.png" alt="See tecnicos" />
                </div>
            </div>
        </div>
    </div>
</main>